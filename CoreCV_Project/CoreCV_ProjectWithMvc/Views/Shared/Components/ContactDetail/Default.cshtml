@using CoreCV_Project.EntityLayer.Concrete
@model List<Contact>

<div class="col-md-6" data-aos="fade-left" data-aos-delay="300">
    @foreach (var item in @Model)
    {
        <div class="mt-3 px-1">
            <div class="h5">@item.Title</div>
            <p>Çalışmalarımı beğendiyseniz ve hizmetlerimden yararlanmak istiyorsanız, iletişim formunu kullanarak bana bir mesaj bırakın.</p>

            <p>Veya e-postamı veya iletişim numaramı kullanarak iletişime geçin.</p>

            <p>Görüşürüz!</p>

        </div>
        <div class="mt-53 px-1">
            <div class="row">
                <div class="col-sm-2">
                    <div class="pb-1">Email:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">@item.Mail</div>
                </div>
                <div class="col-sm-2">
                    <div class="pb-1">Phone:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">@item.Phone</div>
                </div>
            </div>
        </div>
    }
</div>
